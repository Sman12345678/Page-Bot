{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/api", "dest": "app.py" },
    { "src": "/webhook", "dest": "app.py" },
    { "src": "/status", "dest": "app.py" },
    { "src": "/history", "dest": "app.py" },
    { "src": "/", "dest": "app.py" }
  ],
  "env": {
    "PAGE_ACCESS_TOKEN": "@page-access-token",
    "VERIFY_TOKEN": "@verify-token",
    "ADMIN_ID": "@admin-id",
    "PREFIX": "/"
    // Add all your other environment variables here.
  }
}
